        <!--    ########## Lista persone ##########    -->

        <listPerson>

          <person xml:id="Adamo">
            <persName xml:lang="lat">Adamus</persName>
            <sex>M</sex>
            <occupation>Progenitore del genere umano</occupation>
            <note>Adamo è il nome, secondo l'ebraismo, il cristianesimo e l'islam, del primo uomo. Nel
                <emph>Dialogus</emph> l'innocenza di Adamo è difesa da Ludovico Foscarini. </note>
          </person>
          <person xml:id="Eva">
            <persName xml:lang="lat">Eva</persName>
            <sex>F</sex>
            <occupation>Progenitrice del genere umano</occupation>
            <note>Nel racconto biblico di Genesi 2-5, Eva è presentata come la prima donna, nonché progenitrice del
              genere umano. Nel <emph>Dialogus</emph> l'innocenza di Eva è difesa da Isotta Nogarola. </note>
          </person>

        </listPerson>

